{
  "name": "slidev-theme-csscade",
  "version": "0.0.3",
  "type": "module",
  "author": "Manon Carbonnel",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png --dark"
  },
  "dependencies": {
    "@slidev/types": "^0.49.11",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.5",
    "sass": "^1.77.8",
    "sass-embedded": "^1.89.0"
  },
  "devDependencies": {
    "@slidev/cli": "^0.49.11",
    "playwright-chromium": "^1.44.1",
    "prettier": "^3.3.2",
    "stylelint": "^16.6.1",
    "stylelint-config-recommended": "^14.0.1"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Poppins",
        "mono": "Fira Code"
      }
    }
  }
}
